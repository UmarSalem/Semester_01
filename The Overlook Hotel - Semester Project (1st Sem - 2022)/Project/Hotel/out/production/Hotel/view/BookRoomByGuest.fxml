<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="346.0" prefWidth="262.0"
            style="-fx-background-color: #f2ebd3;" xmlns="http://javafx.com/javafx/16"
            fx:controller="view.BookRoomByGuestController">
   <children>
      <TextField fx:id="firstname" layoutX="54.0" layoutY="102.0" promptText="firstname"/>
      <TextField fx:id="lastname" layoutX="54.0" layoutY="154.0" promptText="lastname"/>
      <TextField fx:id="nationality" layoutX="54.0" layoutY="206.0" promptText="nationality"/>
      <TextField fx:id="phoneNumber" layoutX="54.0" layoutY="260.0" promptText="phoneNumber"/>
      <Label layoutX="29.0" layoutY="36.0" text="Enter the details of the guest">
         <font>
            <Font name="Elephant" size="14.0"/>
         </font>
      </Label>
      <Label layoutX="29.0" layoutY="77.0" text="First Name :"/>
      <Label layoutX="29.0" layoutY="128.0" prefHeight="18.0" prefWidth="62.0" text="Last Name :"/>
      <Label layoutX="33.0" layoutY="188.0" text="Nationality :"/>
      <Label layoutX="33.0" layoutY="242.0" text="Phone Number :"/>
      <Button layoutX="97.0" layoutY="306.0" mnemonicParsing="false" onAction="#book" text="Book"/>
      <Button layoutX="167.0" layoutY="306.0" mnemonicParsing="false" onAction="#cancel" text="Cancel"/>
   </children>
</AnchorPane>
